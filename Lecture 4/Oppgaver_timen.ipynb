   "source": [
    "function f(x)\n",
    "    return log(x)\n",
    "end\n",
    "\n",
    "function aprox(f,x,h)\n",
    "    while h>0.000001\n",
    "        x_aprox = (f(x+h) - f(x))/h\n",
    "        println(\"When h is \", h, \" the aproximation is \", x_aprox)\n",
    "        h = h*0.75\n",
    "    end\n",
    "end\n",
    "\n",
    "aprox(f,1,1)\n",
    "\n"
   ]
